{
    "name": "blink/passport",
    "description": "A passport plugin for the Blink framework",
    "keywords": [
        "blink",
        "passport",
        "oauth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jin Hu",
            "email": "bixuehujin@gmail.com"
        }
    ],
    "require": {
        "league/oauth2-server": "^5.1",
        "blink/redis": "dev-master",
        "zendframework/zend-diactoros": "1.3.*"
    },
    "require-dev": {
        "blink/blink": "dev-session-v2",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "blink\\passport\\": "src",
            "blink\\passport\\tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
